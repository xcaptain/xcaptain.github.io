    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "netlify-cms": "^2.9.3",
    "node-sass": "^4.12.0",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
